<script>

let text_container = document.querySelectorAll('.choose_text');

let text_container_2 = document.querySelectorAll('.choose_text-2');
let image_container = document.querySelectorAll('.choose-section');
let swap_btn = document.querySelector('.swap_shop')
for(let i=0; i<text_container.length; i++) {
  text_container[i].addEventListener('click', function(){

    for(let j=0; j<text_container.length; j++) {
      if(j == i) {
        text_container[j].style.borderBottom = '1px solid black'
        image_container[j].style.visibility = 'visible';
        image_container[j].style.height = '100%';
        image_container[j].style.marginBottom = '50px';
        swap_btn.href = pick_collection(j);
        text_container_2[j].style.textDecoration="underline"
      } else {
        text_container[j].style.borderBottom = '0'
        image_container[j].style.visibility = 'hidden'
        image_container[j].style.height = '0';
        image_container[j].style.marginBottom = '0';
        text_container_2[j].style.textDecoration="unset"
      }
    }
  })
}


function pick_collection(num){
    switch(num){
      case 0:
        return '/collections/necklaces'
      case 1:
        return '/collections/earrings'
      case 2:
        return '/collections/rings'
      case 3:
        return '/collections/bracelets'
    }

}
/*text_container[0].click()*/



if(window.innerWidth > 750) {

  $('.slider-nav').slick({
    slidesToShow: 4,
    slidesToScroll: 1,
    /*asNavFor: '.slider-for',*/
    dots: true,
    focusOnSelect: true
  });
  $('a[data-slide]').click(function(e) {
    e.preventDefault();
    let slideno = $(this).data('slide');
    $('.slider-nav').slick('slickGoTo', slideno - 1);
  });

  
var $stage = $('.stage'),
  $textA = $('#customerTextA'),
  $textB = $('#customerTextB'),
  $textC = $('#customerTextC'),
  $textD = $('#customerTextD'),
  $textE = $('#customerTextE'),
  $pointerA = $('#pointer1'),
  $pointerB = $('#pointer2'),
  $pointerC = $('#pointer3'),
  $pointerD = $('#pointer4'),
  $pointer_padding = $('#how-it-works')
  

  // initialize controller
  var controller = new ScrollMagic.Controller();
  controller.scrollTo(0);

  // build timeline with animation tweens
  var scrollAnimation = new TimelineMax();
  scrollAnimation.set($stage, {autoAlpha: 1})
  .to($textA, 0, { css:{className:'+=foundit'} })
  .to($pointerA, 0, { css:{className:'+=current-point'} })

  .to($stage, 1, { css:{className:'+=flx-al-center'} })

  .to($textA, 0, { css:{className:'-=foundit'}}, 1)
  .to($textB, 0, { css:{className:'+=foundit'} }, 1)
  .to($pointerB, 0, { css:{className:'+=current-point'}}, 1)
  .to($pointerA, 0, { css:{className:'-=current-point'} }, 1)

  .to($textB, 0, { css:{className:'-=foundit'}}, 2)
  .to($textC, 0, { css:{className:'+=foundit'}}, 2)
  .to($pointerC, 1, { css:{className:'+=current-point'} }, 2)
  .to($pointerB, 1, { css:{className:'-=current-point'} }, 2)
  //.to($pointer_padding, 1, { css:{className:'-=relative-pos'} }, 2)




  // build scene and link scrolling to animation
  var scene = new ScrollMagic.Scene({triggerElement: ".stage", duration: 2500})
  .setPin(".stage")
  .addTo(controller)
  .setTween(scrollAnimation);
  scene.offset(450);
}

  if(window.innerWidth < 750) {
    $(window).scroll(function(e){ 
      var $el = $('.fixedElement'); 
      var isPositionFixed = ($el.css('position') == 'fixed');
      if ($(this).scrollTop() > (document.querySelector('.shopify-section--slideshow').offsetHeight + document.querySelector('.calc-cont').offsetHeight + document.querySelector('.choose_wrapper-mb').offsetHeight) && !isPositionFixed + 80){ 
        $el.css({'position': 'fixed', 'top': '60px','display':'flex'}); 
      }
      if ($(this).scrollTop() < (document.querySelector('.shopify-section--slideshow').offsetHeight + document.querySelector('.calc-cont').offsetHeight + document.querySelector('.choose_wrapper-mb').offsetHeight) && isPositionFixed + 80){
        $el.css({'position': 'static', 'top': '60px','display':'none'}); 
      } 
    });
  }


</script>





