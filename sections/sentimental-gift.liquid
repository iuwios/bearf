<script>
  var autoslider = autoslider || {
  el: {
    slides: document.getElementsByClassName('slide-1'),
    dots: document.getElementsByClassName('dot')
  },
  defaultDuration: 3500,
  init: function () {
    console.log('ini');
    this.current = 0;
    this.max = this.el.slides.length - 1;
    this.timer = this.getTimerValue(this.current);
    this.setTimer;
    this.initBtnDots();
    this.next();
  },
  initBtnDots: function () {
    var that = this;
    for(let i=0; i< this.el.dots.length; i++) {
      this.el.dots[i].addEventListener('click', function(){
        that.changeSlide(i);
        that.current = i;
        that.stopFunction(that.setTimer)
        that.next();
      })
    }
  },
  stopFunction: function(curTimer) {
    clearTimeout(curTimer);
  },
  next: function () {
    var that = this,
        nextIndex = this.current + 1;
    if (this.current === this.max) {
      nextIndex = 0;
    }
    this.timer = this.getTimerValue(this.current);
    that.setTimer = setTimeout(function () {
      that.changeSlide(nextIndex);
      that.current = nextIndex;
      that.next();
    }, this.timer, nextIndex);
  },
  changeSlide: function (nextIndex) {
    this.el.slides[this.current].classList.remove('active');
    this.el.slides[nextIndex].classList.add('active');
    this.el.dots[this.current].style.fill = '#D5D4D4';
    this.el.dots[nextIndex].style.fill = '#C0BFBF';
  },
  getTimerValue: function (index) {
    return this.el.slides[index].dataset.duration || this.defaultDuration;
  }
};
autoslider.init();
</script>





